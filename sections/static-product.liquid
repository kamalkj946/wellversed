<div class="product-sticky-bar opacitychange">
  <div class="product-sticky-flex-box">
    <div class="product-sticky-title-box">
      <div class="product-sticky-title-container">
        {% comment %} image div  {% endcomment %}
        <div class="product-sticky-image-box">
          <div class="product-sticky-image">
            {{ product.featured_image | img_tag: '', '', 'original' }}
          </div>
        </div>
        {% comment %} title and price div  {% endcomment %}
        <div class="product-sticky-title">
          <p>{{ product.title | escape }}</p>
          <span class="price">{{ product.price | money }}</span>
        </div>

        {% comment %} variant quantity and add to cart div  {% endcomment %}
        <div class="sticky-details">
          <div class="sticky-variants">
            {% if product.variants.size > 1 %}
              <label for="variant-selector">Select Variant:</label>
              <select id="variant-selector">
                {% for variant in product.variants %}
                  <option
                    value="{{ variant.id }}"
                    data-compare-price="{{ variant.compare_at_price | money }}"
                    data-sale-price="{{ variant.price | money }}"
                    variant-img="{{ variant.featured_image.src | product_img_url: 'medium' }}"
                  >
                    {{ variant.title }}
                  </option>
                {% endfor %}
              </select>
              <p id="selectedOption" style="display: none;"></p>
            {% endif %}
          </div>

          <!-- Static Quantity -->
          <div class="quantity-box">
            <button class="quantity-minus">-</button>
            <input type="text" class="quantity-input" value="1" readonly max="10">
            <button class="quantity-plus">+</button>
          </div>
          <!-- Static Quantity -->

          <div class="product-sticky-add-to-cart-box">
            {% render 'gokwik-buy-now' %}
            <div class="product-sticky-add-to-cart">
              <div class="product-form--atc">
                <div class="product-form__action-wrapper">







