<!-- FAQ META FIELDS STARTS -->

<div class="custom_boxed col-md-6 col-sm-12 offset-md-3 mt-5 mb-5 px-3">
  {% if product.metafields.custom.faqtitle1 != blank %}
    <h2 class="heading fw-bold">FAQ's</h2>
  {% endif %}

  <div class="accordion mt-3" id="accordionExample">
    {% assign prefix = 'faqtitle' %}
    {% assign num_fields = 5 %}

    {% for i in (1..num_fields) %}
      {% assign title_key = prefix | append: i %}
      {% assign desc_key = 'faqdesc' | append: i %}

      {% if product.metafields.custom[title_key] != blank and product.metafields.custom[desc_key] != blank %}
        <div class="accordion-item">
          <h3 class="accordion-header" id="heading{{ i }}">
            <button
              class="accordion-button"
              type="button"
              data-bs-toggle="collapse"
              data-bs-target="#collapse{{ i }}"
              aria-expanded="true"
              aria-controls="collapse{{ i }}"
            >
              <b>{{ product.metafields.custom[title_key] }}</b>
            </button>
          </h3>

          <div
            id="collapse{{ i }}"
            class="accordion-collapse collapse"
            aria-labelledby="heading{{ i }}"
            data-bs-parent="#accordionExample"
          >
            <div class="accordion-body">
              <p>{{ product.metafields.custom[desc_key] }}</p>
            </div>
          </div>
        </div>
      {% endif %}
    {% endfor %}
  </div>
</div>

<script>
  $(document).ready(function() {
    $('.custom_boxed .accordion-button').on('click', function() {
      // Remove 'imclicked' class from all accordion buttons except the clicked one
      $('c.ustom_boxed .accordion-button').not(this).removeClass('imclicked');

      // Toggle 'imclicked' class on the clicked button
      $(this).toggleClass('imclicked');
    });
  });
</script>

<!-- FAQ META FIELDS ENDS -->

